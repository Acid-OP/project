{
  "name": "@repo/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "db:generate": "prisma generate --schema=prisma/supabase.prisma && prisma generate --schema=prisma/timescale.prisma",
    "db:supabase:migrate": "prisma migrate dev --schema=prisma/supabase.prisma",
    "db:timescale:migrate": "prisma migrate dev --schema=prisma/timescale.prisma",
    "db:studio:supabase": "prisma studio --schema=prisma/supabase.prisma",
    "db:studio:timescale": "prisma studio --schema=prisma/timescale.prisma",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "6.13.0"
  }
}
